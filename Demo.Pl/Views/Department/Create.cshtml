@model Department
@{
    ViewData["Title"] = "Create";
}

<h1>Create New Department</h1>

<br />
<br />

<form asp-action="Create" asp-controller="Department">
    <div class="form-group">
        <label class="col-form-label" asp-for="Code"></label>
        <input  class="form-control" asp-for="Code"/>
        <span asp-validation-for="Code" class="text"></span>
    </div>
        <div class="form-group">
        <label class="col-form-label" asp-for="Name"></label>
        <input  class="form-control" asp-for="Name"/>
        <span asp-validation-for="Name" class="text"></span>
    </div>
        <div class="form-group">
        <label class="col-form-label" asp-for="CreatedDate"></label>
        <input  class="form-control" asp-for="CreatedDate"/>
        <span asp-validation-for="CreatedDate" class="text"></span>
    </div>
    <input  type="submit"  class="btn btn-primary" value="Submit"/> |
    <a asp-action="Index" asp-controller="Department"  class="btn btn-warning"> Back To List</a>
</form>